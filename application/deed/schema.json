{
 "definitions": {
     "title_number": {
       "pattern": "^([A-Z]{0,3}[1-9][0-9]{0,5}|[0-9]{1,6}[ZT])$",
       "type": "string"
     },
   "PrivateIndividualName": {
           "type": "object",
           "properties": {
               "surname": {
                   "type": "string",
                   "pattern": "^[A-z]+$"
               },
               "forename": {
                   "type": "string",
                    "pattern": "^[A-z]+$"
               },
               "middle_name": {
                   "type": "string"
               },
               "dob": {
                 "type": "string",
                 "pattern": "^(0?[1-9]|[12][0-9]|3[01])[\\/\\-](0?[1-9]|1[012])[\/\\-]\\d{4}$"
               },
               "gender": {
                 "type": "string",
                 "pattern": "^[MFU]$"
               },
             "phone_number": {
               "type": "string",
               "pattern": "^(07[\\d]{8,12}|447[\\d]{7,11})$"
             },
           "address":{
                 "type": "string",
                 "pattern": "(GIR\\s?0AA|[A-Pa-pR-Ur-uWwYyZz]([0-9][0-9A-Ha-hJjKkPpS-Us-uWw]?|[A-Ha-hK-Yk-y][0-9][0-9AaBbEeHhMmNnPpRrV-Yv-y]?)\\s?[0-9][AaBbD-Hd-hJjLlNnP-Up-uW-Zw-z]{2})"
               }
           },
           "required": [
               "surname","forename", "dob", "phone_number", "address"
           ],
           "additionalProperties": false
       }
  },
  "properties": {
    "title_number": {
      "$ref": "#/definitions/title_number"
    },
    "borrowers":{
      "type": "array",
      "minItems": 1,
      "items":
        {
          "$ref": "#/definitions/PrivateIndividualName"
        }

    }
  },
  "required": ["title_number", "borrowers"],
  "type": "object",
  "additionalProperties": false
}